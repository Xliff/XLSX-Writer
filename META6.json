{
    "perl": "6.c",
    "name": "XLSX::Writer",
    "version": "0.1.0",
    "description": "Generates Excel spreadsheets using the libxlsxwriter C library",
    "authors": ["Evan Miller"],
    "license": "MIT",
    "provides": {
        "XLSX::Writer"                       : "lib/XLSX/Writer.pm6",
        "XLSX::Writer::Common"               : "lib/XLSX/Writer/Common.pm6",
        "XLSX::Writer::Align"                : "lib/XLSX/Writer/Align.pm6",
        "XLSX::Writer::Border"               : "lib/XLSX/Writer/Border.pm6",
        "XLSX::Writer::Pattern"              : "lib/XLSX/Writer/Pattern.pm6",
        "XLSX::Writer::DateTime"             : "lib/XLSX/Writer/DateTime.pm6",
